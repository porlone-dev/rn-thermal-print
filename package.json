{
  "name": "@porlone/rn-thermal-print",
  "version": "2.0.0",
  "description": "A React Native Library to support BLE printer with New Architecture support",
  "main": "dist/index.js",
  "source": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/porlone-dev/rn-thermal-print"
  },
  "files": [
    "dist",
    "android",
    "ios",
    "specs",
    "rn-thermal-print.podspec",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "keywords": [
    "react-native",
    "thermal",
    "printer",
    "bluetooth",
    "net",
    "printer",
    "rn",
    "android",
    "image",
    "qr",
    "base64",
    "receipt",
    "bill",
    "invoice",
    "turbo-module",
    "new-architecture"
  ],
  "dependencies": {
    "buffer": "^6.0.3",
    "iconv-lite": "^0.6.3",
    "string_decoder": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": ">=0.70.0",
    "@porlone/rn-ping": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@porlone/rn-ping": {
      "optional": true
    }
  },
  "codegenConfig": {
    "name": "RNThermalPrinterSpec",
    "type": "modules",
    "jsSrcsDir": "specs",
    "android": {
      "javaPackageName": "com.pinmi.react.printer.turbo"
    }
  },
  "author": "porlone.agency@gmail.com",
  "homepage": "https://github.com/porlone-dev/rn-thermal-print#readme",
  "license": "ISC"
}
